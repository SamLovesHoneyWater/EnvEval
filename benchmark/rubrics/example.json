{
  "repo": "example_project",
  "tests": [
    {
      "id": "check_java",
      "type": "commands_exist",
      "params": {
        "names": ["java"]
      }
    },
    {
      "id": "check_java_version",
      "type": "output_contains",
      "params": {
        "command": "java -version",
        "contains": ["11", "12", "13", "17"]
      },
      "timeout": 5,
      "requires": ["check_java"],
      "score": 2
    },
    {
      "id": "check_build_file",
      "type": "files_exist",
      "params": {
        "paths": [
          "build.gradle"
        ]
      },
      "score": 1
    },
    {
      "id": "check_spec_dir",
      "type": "dirs_exist",
      "params": {
        "paths": [
          "spec"
        ]
      },
      "score": 1
    },
    {
      "id": "check_java_home",
      "type": "envvar_set",
      "params": {
        "name": "JAVA_HOME"
      },
      "score": 2
    },
    {
      "id": "check_ruby_version",
      "type": "file_contains",
      "params": {
        "path": ".ruby-version",
        "contains": ["jruby-9.2", "jruby-9.3"]
      },
      "score": 1
    },
    {
      "id": "check_jruby_gem",
      "type": "run_command",
      "params": {
        "command": "jruby -S gem -v"
      },
      "timeout": 60,
      "score": 2
    }
  ]
}
