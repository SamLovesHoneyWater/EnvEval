      "params": {
        "command": "cargo clippy --version"
      },
      "timeout": 15,
      "score": 1,
      "category": "configuration"
    },
    {
      "id": "check_rustfmt_available",
      "type": "commands_exist",
      "params": {
        "names": [
          "rustfmt"
        ]
      },
      "score": 1,
      "category": "configuration"
    },
