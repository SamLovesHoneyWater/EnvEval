{
  "repo": "serde-rs_serde",
  "tests": [
    {
      "id": "check_system_tools",
      "type": "commands_exist",
      "params": {
        "names": [
          "git",
          "curl",
          "make",
          "gcc"
        ]
      },
      "score": 3,
      "category": "configuration"
    },
    {
      "id": "check_rust_compiler",
      "type": "commands_exist",
      "params": {
        "names": [
          "rustc",
          "cargo"
        ]
      },
      "score": 4,
      "category": "configuration"
    },
    {
      "id": "check_rust_version_compatible",
      "type": "output_contains",
      "params": {
        "command": "rustc --version",
        "contains": [
          "1.56",
          "1.57",
          "1.58",
          "1.59",
          "1.6",
          "1.7",
          "1.8"
        ]
      },
      "timeout": 10,
      "score": 3,
      "category": "configuration"
    },
    {
      "id": "check_cargo_version",
      "type": "run_command",
      "params": {
        "command": "cargo --version"
      },
      "timeout": 10,
      "score": 2,
      "category": "configuration"
    },
    {
      "id": "check_pkg_config",
      "type": "commands_exist",
      "params": {
        "names": [
          "pkg-config"
        ]
      },
      "score": 1,
      "category": "configuration"
    },
    {
      "id": "check_openssl_dev",
      "type": "run_command",
      "params": {
        "command": "pkg-config --exists openssl"
      },
      "timeout": 10,
      "score": 2,
      "category": "configuration"
    },
    {
      "id": "check_rustup_available",
