      "params": {
        "command": "cd test_suite && cargo test test_macros --quiet"
      },
      "timeout": 180,
      "score": 3,
      "category": "functionality"
    },
    {
      "id": "test_cargo_workspace_build",
      "type": "run_command",
      "params": {
        "command": "cargo build --workspace --quiet"
      },
      "timeout": 600,
      "score": 5,
      "category": "functionality"
    },
    {
      "id": "test_serde_serialization_example",
      "type": "run_command",
      "params": {
        "command": "cd serde && cargo run --example serialize"
      },
      "timeout": 60,
      "score": 3,
      "category": "functionality"
    },
    {
      "id": "test_documentation_build",
      "type": "run_command",
      "params": {
        "command": "cargo doc --package serde --no-deps --quiet"
      },
      "timeout": 300,
      "score": 2,
      "category": "functionality"
    },
    {
      "id": "test_clippy_check",
      "type": "run_command",
      "params": {
        "command": "cargo clippy --package serde --quiet -- -A warnings"
      },
      "timeout": 180,
      "score": 2,
      "category": "functionality"
    },
    {
      "id": "test_no_std_compilation",
      "type": "run_command",
      "params": {
        "command": "cd test_suite/no_std && cargo build --quiet"
      },
      "timeout": 120,
      "score": 3,
      "category": "functionality"
    }
  ]
}