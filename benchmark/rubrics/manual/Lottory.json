      },
      "timeout": 30,
      "score": 1,
      "category": "configuration"
    },
    {
      "id": "test_numpy_import",
      "type": "run_command",
      "params": {
        "command": "python3.7 -c \"import numpy; print('numpy works')\""
      },
      "timeout": 30,
