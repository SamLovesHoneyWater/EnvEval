{
  "repo": "Fairify",
  "tests": [
    {
      "id": "check_system_tools",
      "type": "commands_exist",
      "params": {
        "names": ["python3", "pip3", "git", "bash", "curl", "wget"]
      },
      "score": 2
    },
    {
      "id": "check_python_version",
      "type": "output_contains",
      "params": {
        "command": "python3 --version",
        "contains": ["Python 3."]
      },
      "timeout": 10,
      "score": 2
    },
    {
      "id": "check_core_files",
