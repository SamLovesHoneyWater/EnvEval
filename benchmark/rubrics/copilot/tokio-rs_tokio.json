{
  "repo": "tokio-rs_tokio",
  "tests": [
    {
      "id": "check_rust_toolchain",
      "type": "commands_exist",
      "params": {
        "commands": ["rustc", "cargo"]
      },
      "score": 3,
      "category": "configuration"
    },
    {
      "id": "check_rust_version",
      "type": "output_contains",
      "params": {
        "command": "rustc --version",
        "expected_strings": ["rustc 1."]
      },
      "timeout": 10,
      "score": 2,
      "category": "configuration"
    },
    {
      "id": "check_cargo_version",
      "type": "run_command",
      "params": {
        "command": "cargo --version"
      },
      "timeout": 10,
      "score": 1,
      "category": "configuration"
    },
    {
      "id": "check_build_tools",
      "type": "commands_exist",
      "params": {
        "commands": ["git", "gcc", "make"]
      },
      "score": 2,
      "category": "configuration"
    },
    {
      "id": "check_workspace_structure",
      "type": "files_exist",
      "params": {
        "files": ["Cargo.toml", "README.md", "LICENSE", "Cross.toml", "deny.toml"]
      },
      "score": 2,
      "category": "structure"
    },
    {
      "id": "check_main_directories",
      "type": "dirs_exist",
      "params": {
        "dirs": ["tokio", "tokio-util", "tokio-test", "tokio-stream", "tokio-macros"]
      },
      "score": 3,
      "category": "structure"
    },
    {
      "id": "check_development_directories",
      "type": "dirs_exist",
      "params": {
        "dirs": ["examples", "benches", "tests-integration", "tests-build"]
      },
      "score": 2,
      "category": "structure"
    },
    {
      "id": "check_tokio_crate_files",
      "type": "files_exist",
      "params": {
        "files": ["tokio/Cargo.toml", "tokio/src/lib.rs", "tokio/README.md"]
      },
      "score": 2,
      "category": "structure"
    },
    {
      "id": "check_config_files",
      "type": "files_exist",
      "params": {
        "files": ["spellcheck.toml", "spellcheck.dic", "SECURITY.md", "CODE_OF_CONDUCT.md", "CONTRIBUTING.md"]
      },
      "score": 1,
      "category": "structure"
    },
    {
      "id": "test_cargo_check",
      "type": "run_command",
      "params": {
        "command": "cargo check --workspace"
      },
      "timeout": 120,
      "score": 3,
      "category": "configuration",
      "requires": ["check_rust_toolchain"]
    },
    {
      "id": "test_cargo_clippy",
      "type": "run_command",
      "params": {
        "command": "cargo clippy --workspace --all-targets --all-features -- -D warnings"
      },
      "timeout": 180,
      "score": 2,
      "category": "configuration",
      "requires": ["check_rust_toolchain"]
    },
    {
      "id": "test_tokio_main_crate_build",
      "type": "run_command",
      "params": {
        "command": "cargo build -p tokio --all-features"
      },
      "timeout": 300,
      "score": 4,
      "category": "functionality",
      "requires": ["check_rust_toolchain", "test_cargo_check"]
    },
    {
      "id": "test_tokio_macros_build",
      "type": "run_command",
      "params": {
        "command": "cargo build -p tokio-macros"
      },
      "timeout": 60,
      "score": 2,
      "category": "functionality",
      "requires": ["check_rust_toolchain"]
    },
    {
      "id": "test_tokio_util_build",
      "type": "run_command",
      "params": {
        "command": "cargo build -p tokio-util"
      },
      "timeout": 120,
      "score": 2,
      "category": "functionality",
      "requires": ["check_rust_toolchain"]
    },
    {
      "id": "test_tokio_stream_build",
      "type": "run_command",
      "params": {
        "command": "cargo build -p tokio-stream"
      },
      "timeout": 90,
      "score": 2,
      "category": "functionality",
      "requires": ["check_rust_toolchain"]
    },
    {
      "id": "test_tokio_test_build",
      "type": "run_command",
      "params": {
        "command": "cargo build -p tokio-test"
      },
      "timeout": 90,
      "score": 2,
      "category": "functionality",
      "requires": ["check_rust_toolchain"]
    },
    {
      "id": "test_workspace_build",
      "type": "run_command",
      "params": {
        "command": "cargo build --workspace"
      },
      "timeout": 600,
      "score": 5,
      "category": "functionality",
      "requires": ["check_rust_toolchain", "test_cargo_check"]
    },
    {
      "id": "test_examples_compile",
      "type": "run_command",
      "params": {
        "command": "cargo build --examples -p tokio"
      },
      "timeout": 300,
      "score": 3,
      "category": "functionality",
      "requires": ["test_tokio_main_crate_build"]
    },
    {
      "id": "test_basic_tokio_usage",
      "type": "run_command",
      "params": {
        "command": "cargo run --example hello-world -p tokio"
      },
      "timeout": 60,
      "score": 3,
      "category": "functionality",
      "requires": ["test_tokio_main_crate_build"]
    },
    {
      "id": "test_cargo_deny_check",
      "type": "run_command",
      "params": {
        "command": "cargo deny check"
      },
      "timeout": 120,
      "score": 1,
      "category": "configuration",
      "requires": ["check_rust_toolchain"]
    },
    {
      "id": "test_unit_tests_sample",
      "type": "run_command",
      "params": {
        "command": "cargo test -p tokio --lib --test sleep -- --test-threads=1"
      },
      "timeout": 180,
      "score": 3,
      "category": "functionality",
      "requires": ["test_tokio_main_crate_build"]
    },
    {
      "id": "test_integration_tests_sample",
      "type": "run_command",
      "params": {
        "command": "cargo test -p tokio --test rt_threaded --test-threads=1"
      },
      "timeout": 180,
      "score": 3,
      "category": "functionality",
      "requires": ["test_tokio_main_crate_build"]
    },
    {
      "id": "test_doc_build",
      "type": "run_command",
      "params": {
        "command": "cargo doc --no-deps -p tokio"
      },
      "timeout": 300,
      "score": 2,
      "category": "functionality",
      "requires": ["test_tokio_main_crate_build"]
    },
    {
      "id": "check_rust_source_files",
      "type": "run_command",
      "params": {
        "command": "find . -name '*.rs' | head -10"
      },
      "timeout": 10,
      "score": 1,
      "category": "structure"
    },
    {
      "id": "check_toml_syntax",
      "type": "run_command",
      "params": {
        "command": "cargo metadata --format-version 1 > /dev/null"
      },
      "timeout": 30,
      "score": 1,
      "category": "configuration",
      "requires": ["check_rust_toolchain"]
    },
    {
      "id": "test_feature_flags",
      "type": "run_command",
      "params": {
        "command": "cargo build -p tokio --no-default-features --features rt"
      },
      "timeout": 180,
      "score": 2,
      "category": "functionality",
      "requires": ["check_rust_toolchain"]
    },
    {
      "id": "test_full_feature_build",
      "type": "run_command",
      "params": {
        "command": "cargo build -p tokio --all-features"
      },
      "timeout": 300,
      "score": 3,
      "category": "functionality",
      "requires": ["check_rust_toolchain"]
    },
    {
      "id": "test_cross_compilation_config",
      "type": "file_contains",
      "params": {
        "file": "Cross.toml",
        "strings": ["[build.env]"]
      },
      "score": 1,
      "category": "configuration"
    },
    {
      "id": "test_workspace_config",
      "type": "file_contains",
      "params": {
        "file": "Cargo.toml",
        "strings": ["[workspace]", "resolver = \"2\""]
      },
      "score": 1,
      "category": "configuration"
    },
    {
      "id": "test_tokio_minimum_rust_version",
      "type": "file_contains",
      "params": {
        "file": "tokio/Cargo.toml",
        "strings": ["rust-version = \"1.70\""]
      },
      "score": 1,
      "category": "configuration"
    },
    {
      "id": "test_benchmarks_exist",
      "type": "run_command",
      "params": {
        "command": "find benches -name '*.rs' | head -3"
      },
      "timeout": 10,
      "score": 1,
      "category": "structure"
    }
  ]
}